#parse('/assets/lib/bootstrap/widgets/datepicker/i18n.vm')

<!-- datepicker -->
<div class="input-prepend date">
    <span class="add-on"><i class="icon-th"></i></span>
    <a id="button-<%= cid %>" class="btn" data-date="2012-01-01" data-date-format="yyyy-mm-dd">$click</a>
    <a id="reset-<%= cid %>" class="btn" >$reset</a>
</div>
